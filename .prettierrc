{
	"bracketSpacing": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"plugins": [
		"prettier-plugin-organize-imports",
		"prettier-plugin-svelte",
		"prettier-plugin-tailwindcss"
	],
	"printWidth": 80,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 4,
	"trailingComma": "none",
	"useTabs": true
}
